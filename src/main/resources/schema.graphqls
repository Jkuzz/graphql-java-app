type Query {
    kraje: [Kraj!]!
    okresy: [Okres!]!
    obce: [Obec!]!
    krajById(id: ID): Kraj
    okresById(id: ID): Okres
    obecById(id: ID): Obec
}

type Kraj {
    id: ID!
    name: String!
    NUTS: String!
}

type Okres {
    id: ID!
    name: String!
    NUTS: String!
    KOD_RUIAN: String
    kraj: Kraj
}

type Obec {
    id: ID!
    name: String!
    kraj: Kraj
    okres: Okres
}
