type Query {
    bookById(id: ID): Book
    booksByAuthor(authorId: ID!): [Book!]!
    books: [Book!]!
    kraje: [Kraj!]!
    okresy: [Okres!]!
    obce: [Obec!]!
    krajById(id: ID): Kraj
    okresById(id: ID): Okres
    obecById(id: ID): Obec
}

type Book {
    id: ID!
    name: String!
    pageCount: Int
    author: Author!
}

type Author {
    id: ID!
    firstName: String
    lastName: String
}

type Kraj {
    id: ID!
    name: String!
    NUTS: String!
}

type Okres {
    id: ID!
    name: String!
    NUTS: String!
    KOD_RUIAN: String
    kraj: Kraj
}

type Obec {
    id: ID!
    name: String!
    kraj: Kraj
    okres: Okres
}
